<Window x:Class="Маслов_А.Н._4ПКС_318_Практические.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Маслов_А.Н._4ПКС_318_Практические"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="211*"/>
            <ColumnDefinition Width="581*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition/>
            <RowDefinition Height="42"/>
        </Grid.RowDefinitions>
        <Rectangle  Stroke="Black" Grid.Row="2" Grid.ColumnSpan="2"/>
        <Button x:Name="ButtonBack" Content="Назад"  HorizontalAlignment="Left" Margin="10,10,0,10" Width="75" Click="ButtonBack_Click"/>
        <Frame x:Name="MainFrame" Source="Pages/AuthPage.xaml" Grid.Row="1" Navigated="MainFrame_OnNavigated" NavigationUIVisibility="Hidden" Grid.ColumnSpan="2"/>
        <Button x:Name="ButtonBack_Copy" Content="Калькулятор"  HorizontalAlignment="Left" Margin="496,10,0,10" Width="75" Click="ButtonBack_Copy_Click" Grid.Column="1"/>


    </Grid>
</Window>
