<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Маслов_А.Н._4ПКС_318_Практические">
    
    <Style TargetType="{x:Type local:MainWindow}">
        <Setter Property="Background" Value="#B5D5CA"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Foreground" Value="#D1EEFC"/>
    </Style>
    <Style TargetType="{x:Type Rectangle}">
        <Setter Property="Fill" Value="#D1EEFC"/>
    </Style>
    <Style TargetType="{x:Type Button}">
        <Setter Property="Background" Value="#D1EEFC"/>
    </Style>
    
   
</ResourceDictionary>